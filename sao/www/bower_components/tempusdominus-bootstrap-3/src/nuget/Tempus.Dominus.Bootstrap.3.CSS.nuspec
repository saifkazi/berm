<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Tempus.Dominus.Bootstrap3.LESS</id>
    <version>5.0.0-alpha2</version>
    <title>Tempus Dominus for Bootstrap 3</title>
    <authors>Eonasdan</authors>
    <owners>Eonasdan</owners>
    <projectUrl>https://github.com/tempusdominus/bootstrap-3</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      A date/time picker component designed to work with Bootstrap 3 and Momentjs.

      For usage, installation and demos see Project Site on GitHub

      For CSS version install Tempus.Dominus.Bootstrap3.CSS
    </description>
    <releaseNotes>
      Check the change log on Github at https://tempusdominus.github.io/core/Change-Log
    </releaseNotes>
    <tags>bootstrap date time picker datetimepicker datepicker jquery</tags>
    <dependencies>
      <dependency id="bootstrap.Less" version="3.3.0" />
      <dependency id="Moment.js" version="2.9.0" />
      <dependency id="Tempus.Dominus.Core" version="5.0.0-alpha6" />
    </dependencies>
  </metadata>
	<files>
        <file src="..\..\build\js\tempusdominus-bootstrap-3.js" target="content\Scripts" />
        <file src="..\..\build\js\tempusdominus-bootstrap-3.min.js" target="content\Scripts" />
        <file src="..\..\build\css\tempusdominus-bootstrap-3.css" target="content\Content\" />
        <file src="..\..\build\css\tempusdominus-bootstrap-3.min.css" target="content\Content\" />
    </files>
</package>